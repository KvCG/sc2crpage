{
    "name": "sc2crpage",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently --names \"FE,BE\" -c \"auto,green\" \"vite\" \"nodemon\"",
        "build": "tsc -b & npm run build:client && npm run build:server && node scripts/copyData.cjs",
        "build:server": "node scripts/build.cjs",
        "build:client": "vite build",
        "lint": "eslint .",
        "buildImg": "docker build -t sc2cr .",
        "localPod": "docker run -dp 127.0.0.1:3000:3000 sc2cr",
        "tagImg": "docker tag sc2cr:latest kerverus/sc2cr:latest",
        "pushImg": "npm run buildImg && npm run tagImg && docker push kerverus/sc2cr:latest"
    },
    "dependencies": {
        "@mantine/core": "^7.12.1",
        "@mantine/hooks": "^7.12.1",
        "@tabler/icons-react": "^3.14.0",
        "axios": "^1.7.5",
        "csv-parser": "^3.0.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "firebase-admin": "^12.4.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.26.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/react": "^18.3.4",
        "@types/react-dom": "^18.3.0",
        "@types/ws": "^8.5.12",
        "@vitejs/plugin-react": "^4.3.1",
        "concurrently": "^8.2.2",
        "cors": "^2.8.5",
        "esbuild": "^0.23.1",
        "eslint": "^9.9.0",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.9",
        "fs-extra": "^11.2.0",
        "globals": "^15.9.0",
        "nodemon": "^3.1.4",
        "postcss": "^8.4.41",
        "postcss-preset-mantine": "^1.17.0",
        "postcss-simple-vars": "^7.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.0.1",
        "vite": "^5.4.1",
        "ws": "^8.18.0"
    }

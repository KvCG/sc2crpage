{
    "name": "sc2crpage",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "concurrently --names \"FE,BE\" -c \"auto,green\" \"vite\" \"nodemon\"",
        "build": "tsc -b & npm run build:client && npm run build:server",
        "build:server": "node scripts/build.cjs",
        "build:client": "vite build",
        "lint": "eslint .",
        "buildImg": "docker build -t sc2cr .",
        "localPod": "docker run -dp 127.0.0.1:3000:3000 sc2cr",
        "tagImg": "docker tag sc2cr:latest kerverus/sc2cr:latest",
        "pushImg": "npm run buildImg && npm run tagImg && docker push kerverus/sc2cr:latest",
        "start": "node dist/webserver/server.cjs",
        "test": "vitest run -c vitest.client.config.ts && vitest run -c vitest.server.config.ts",
        "test:watch": "vitest -c vitest.client.config.ts",
        "test:client": "vitest run -c vitest.client.config.ts",
        "test:server": "vitest run -c vitest.server.config.ts",
        "coverage": "vitest run -c vitest.client.config.ts --coverage && vitest run -c vitest.server.config.ts --coverage"
    },
    "dependencies": {
        "@mantine/core": "^7.17.3",
        "@mantine/hooks": "^7.17.3",
        "@tabler/icons-react": "^3.14.0",
        "@vercel/analytics": "^1.3.1",
        "@vercel/speed-insights": "^1.0.12",
        "axios": "^1.7.5",
        "csv-parser": "^3.0.0",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "firebase-admin": "^12.4.0",
        "googleapis": "^144.0.0",
        "lru-cache": "^11.1.0",
        "luxon": "^3.6.1",
        "node-cache": "^5.1.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.26.1",
        "pino": "^9.4.0",
        "pino-http": "^10.3.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@testing-library/jest-dom": "^6.4.8",
        "@testing-library/react": "^14.2.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/luxon": "^3.6.2",
        "@types/react": "^18.3.4",
        "@types/react-dom": "^18.3.0",
        "@types/ws": "^8.5.12",
        "@vitejs/plugin-react": "^4.3.1",
        "@vitest/coverage-v8": "^2.1.9",
        "concurrently": "^8.2.2",
        "cors": "^2.8.5",
        "esbuild": "^0.23.1",
        "eslint": "^9.9.0",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.9",
        "fs-extra": "^11.2.0",
        "globals": "^15.9.0",
        "jsdom": "^24.0.0",
        "msw": "^1.3.3",
        "nodemon": "^3.1.4",
        "postcss": "^8.4.41",
        "postcss-preset-mantine": "^1.17.0",
        "postcss-simple-vars": "^7.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.0.1",
        "vite": "^5.4.1",
        "vitest": "^2.0.5",
        "ws": "^8.18.0",
        "pino-pretty": "^11.2.2",
        "supertest": "^7.0.0"
    }
}

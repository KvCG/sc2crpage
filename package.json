{
    "name": "sc2crpage",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && npm run build:client && npm run build:server",
        "build:server": "node scripts/build.cjs",
        "build:client": "vite build",
        "lint": "eslint .",
        "preview": "npm run build:server && node dist/webserver/server.cjs",
        "ds": "tsc --project src/server/ && npm run renameFiles && nodemon --env-file=.env src/server/server.cjs",
        "renameFiles": "powershell -Command \"Move-Item -Path 'src/server/server.js' -Destination 'src/server/server.cjs' -Force\"",
        "buildImg": "docker build --no-cache -t sc2cr .",
        "localPod": "docker run -dp 127.0.0.1:3000:3000 sc2cr",
		"tagImg": "docker tag sc2cr:latest kerverus/sc2cr:latest",
		"pushImg": "npm run buildImg && npm run tagImg && docker push kerverus/sc2cr:latest"
    },
    "dependencies": {
        "axios": "^1.7.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.9.0",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/react": "^18.3.4",
        "@types/react-dom": "^18.3.0",
        "@vitejs/plugin-react": "^4.3.1",
        "cors": "^2.8.5",
        "esbuild": "^0.23.1",
        "eslint": "^9.9.0",
        "eslint-plugin-react-hooks": "^5.1.0-rc.0",
        "eslint-plugin-react-refresh": "^0.4.9",
        "globals": "^15.9.0",
        "nodemon": "^3.1.4",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.3",
        "typescript-eslint": "^8.0.1",
        "vite": "^5.4.1"
    }
}
